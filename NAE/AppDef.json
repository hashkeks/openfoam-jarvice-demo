{
	"name": "OpenFoam&reg; 8 - Prakti Edition",
	"description": "OpenFoam8 with Nimbix-Desktop",
	"author": "Casper, Kevin",
	"licensed": false,
	"classifications": [
		"Stroemungssimulation"
	],
	"machines": [
		"*"
	],
	"vault-types": [
		"FILE"
	],
	"commands": {
		"Batch": {
			"path": "/usr/local/scripts/openfoam-batch.sh",
			"interactive": true,
			"name": "OpemFoam Batch",
			"description": "Run OpenFoam Batch",
			"parameters": {
				"-case": {
					"required": true,
					"type": "FILE",
					"name": "OpenFoam case, select any file"
				},
				"-mesh": {
					"type": "BOOL",
					"required": true,
					"value": true,
					"name": "Run mesh generation?",
					"description": "Select here to run the selected mesh generation utility on the Case"
				},
				"-meshtype": {
					"vaules": [
						"blockMesh",
						"snappyHexMesh"
					],
					"required": true,
					"type": "selection",
					"name": "Mesh generator"
				},
				"-solver": {
					"values": [
						"pimpleFoam",
						"fireFoam",
						"financialFoam"
					],
				"required": true,
				"type": "selection",
				"name": "Solver"
				}
			}
		}
	},
	"image": {
		"type": "image/png",
		"data": ""
	}
} 
